{
  "name": "intelijob",
  "version": "3.0.0",
  "description": "Personal cert research tool \u2014 analyzes job postings to rank cybersecurity certifications by employer demand",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "check:fullstack": "npm run type-check && npm run build && python3 -m pytest backend/test_e2e.py",
    "start": "python3 scripts/dev_start.py",
    "start:frontend": "npm run dev",
    "start:backend": "cd backend && python3 start.py"
  },
  "author": "Jacob Macdonnell",
  "license": "MIT",
  "dependencies": {
    "@chakra-ui/icons": "2.1.1",
    "@chakra-ui/react": "2.8.2",
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.5",
    "axios": "^1.9.0",
    "framer-motion": "10.18.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^5.1.4",
    "typescript": "~5.7.2",
    "vite": "^6.2.0"
  }
}
